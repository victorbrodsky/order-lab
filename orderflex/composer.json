{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "asimlqt/php-google-spreadsheet-client": "^3.0",
        "bmatzner/fontawesome-bundle": "^4.7",
        "box/spout": "^3.1",
        "friendsofsymfony/comment-bundle": "^3",
        "friendsofsymfony/jsrouting-bundle": "^2.5",
        "friendsofsymfony/user-bundle": "^2.1",
        "google/apiclient": "^2.4",
        "knplabs/knp-paginator-bundle": "^4.1",
        "knplabs/knp-snappy-bundle": "^1.6",
        "oneup/uploader-bundle": "^2.1",
        "paragonie/random_compat": "^2",
        "phpoffice/phpexcel": "^1.8",
        "phpoffice/phpspreadsheet": "^1.10",
        "sensio/framework-extra-bundle": "^5.5",
        "sinergi/browser-detector": "^6.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/expression-language": "^4.4",
        "symfony/flex": "^1.6",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "^4.4",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/translation": "4.4.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/workflow": "^4.4",
        "symfony/yaml": "^4.4",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Doctrine\\Common\\Lexer\\": "src/App/Doctrine/Common/Lexer/"
        },
		"classmap": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
