            {#getReviewEnabledLinkActions: reviewer={{ review.getReviewer() }}<br>#}
            {% set actionReviewLinks = transres_util.getReviewEnabledLinkActions(review) %}
            {% set actionLinks = actionLinks|merge({ (loop.index0): actionReviewLinks}) %}

